#include <iostream>
using namespace std;


int main(void)
{
	ios::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	int n;
	cin >> n;

	for(int i=1;i<=n;i++)
	{
		if(i%2==0)
		{
			cout << " ";
		}
		for(int j=0;j<n*2;j++)
		{
			if((j+1)%2==0)
			{
				cout << " ";
			}
			else
			{
				cout << "*";
			}
		
		}
		cout << "\n";
	}
	
}
