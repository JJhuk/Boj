#include <iostream>
#include <vector>
#include <algorithm>
#include <string>

using namespace std;

int N, K;
int temp;
vector<int> v;

int main(void) {
	ios::sync_with_stdio(false);
	cin >> N >> K;
	for (int i = 0; i < N; i++) {
		cin >> temp;
		v.push_back(temp);
	}
	sort(v.begin(), v.end());

	cout << v[K - 1];

	return 0;
}
