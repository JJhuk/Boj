#include <iostream>
#include <algorithm>
#include <string>
using namespace std;
int test_case;
string s;
int main(void) {
	ios::sync_with_stdio(false);
	cin.tie(0);
	cin >> test_case;
	while (test_case--) {
		cin >> s;
		sort(s.begin(), s.end());
		do {
			cout << s << "\n";
		} while (next_permutation(s.begin(), s.end()));
	}
	system("pause");
	return 0;
}
