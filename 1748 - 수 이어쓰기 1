#include <iostream>
#include <cstring>
#define MAX 100	
int map[MAX][MAX];

using namespace std;

int main(void) {
	ios::sync_with_stdio(false);
	cin.tie(0);
	int n;
	int sum = 0;
	cin >> n;
	for (int i = 1; i <= n; i++) {
		if (i >= 1 && i < 10) {
			sum += 1;
		}
		else if (i >= 10 && i < 100) {
			sum += 2;
		}
		else if (i >= 100 && i < 1000) {
			sum += 3;
		}
		else if (i >= 1000 && i < 10000) {
			sum += 4;
		}
		else if (i >= 10000 && i < 100000) {
			sum += 5;
		}
		else if (i >= 100000 && i < 1000000) {
			sum += 6;
		}
		else if (i >= 1000000 && i < 10000000) {
			sum += 7;
		}
		else if (i >= 10000000 && i < 100000000) {
			sum += 8;
		}
		else if (i ==100000000) {
			sum += 9;
		}
	}
	cout << sum << "\n";
	return 0;
}
